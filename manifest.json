{
  "manifest_version": 3,
  "name": "Click Lemur",
  "version": "1.0",
  "description": "Press CTRL + ' to click Run Code and CTRL + Enter to click Submit.",
  "permissions": ["activeTab", "scripting"],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-16.png",
    "48": "icons/icon-16.png",
    "128": "icons/icon-16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    } 
  ],
  "action": {
    "default_icon": "icons/icon-16.png",
    "default_popup": "popup.html"
  },
  "author": "codexdebayan"
}
